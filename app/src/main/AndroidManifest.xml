<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jo.kisapi">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name=".application.KISApplication"
        android:allowBackup="true"
        android:hardwareAccelerated="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.KISapi"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".composetest.AutoTrading2"
            android:exported="false"
            android:label=""
            android:theme="@style/Theme.KISapi">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".composetest.AutoTrading1"
            android:exported="false"
            android:label=""
            android:theme="@style/Theme.KISapi">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".composetest.MainActivity"
            android:exported="true"
            android:label=""
            android:theme="@style/Theme.KISapi">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".activity.AutoTradingActivity"
            android:exported="true" />
        <activity
            android:name=".activity.OrderActivity"
            android:exported="true" />

        <receiver
            android:name=".AlarmReceiver"
            android:exported="false" />

        <activity
            android:name=".activity.InquireBalanceActivity"
            android:exported="true" />

    </application>

</manifest>